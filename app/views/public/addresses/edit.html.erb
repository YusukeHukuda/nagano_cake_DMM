<h5>配送先編集</h5>

 <%= form_with model: @address, url: addresses_path do |f| %>

  <div>
    <%= f.label :"郵便番号（ハイフンなし)" %>
    <%= f.text_field :postal_code %>
  </div>

  <div>
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :宛名 %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.submit "変更内容を保存" %>
  </div>

  <% end %>
